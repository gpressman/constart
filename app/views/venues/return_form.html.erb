<h3><%=  @request.artist %> </h3>
<ul class="return-form-description" >
	<li><img src="<%= @request.picture%>"></li>
	<li>City: <%=  @request.city  %>
    <li> Date request: <%=  @request.date.strftime("%-m-%-d-%Y") %>
    <li>Description: <%   @request.description %>
</ul>


<%= bootstrap_form_for @request do |f| %>

	
	
	<%= f.date_field :date %>

	<%= f.time_field :time %>

	<%= f.text_field :city %>

	<%= f.text_field :ticket_price %>

	<%= f.text_field :tickets_required %>

	<%= f.date_field :date_campaign_ends %>

	<%= f.submit 'Send Concert' %>
<% end %>